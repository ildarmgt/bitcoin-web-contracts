{
  "name": "webcontracts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && npm run zip",
    "lint": "vue-cli-service lint",
    "zip": "zip -x *.zip -r ./docs/dapp.zip docs"
  },
  "dependencies": {
    "axios": "0.19.0",
    "bip32": "2.0.4",
    "bip39": "3.0.2",
    "bip65": "1.0.3",
    "bip68": "1.0.4",
    "bitcoinjs-lib": "4.0.3",
    "qr-image": "3.2.0",
    "sjcl": "1.0.8",
    "vue": "2.6.11",
    "vue-flash-message": "0.7.2",
    "vue-router": "3.1.3",
    "vuex": "3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.4.6",
    "@vue/cli-plugin-eslint": "4.4.6",
    "@vue/cli-service": "4.4.6",
    "babel-eslint": "10.1.0",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-vue": "6.2.2",
    "semistandard": "14.2.2",
    "vue-template-compiler": "2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "semistandard",
      "plugin:vue/strongly-recommended"
    ],
    "rules": {
      "indent": "warn",
      "camelcase": "off",
      "vue/attribute-hyphenation": "off",
      "prefer-const": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "ignorePatterns": [
      "sjcl.js"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}